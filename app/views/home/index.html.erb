<div class="row">
	<div class="col-md-12">
		<h1>Team Map</h1>
	</div>
</div>

<div class="row">
	<div class="col-md-8 col-md-offset-2">
		<div id="googleMap" style="width:100%;height:380px;"></div>
	</div>
  <div class="col-md-2"></div>
</div>

<div class="row">
	<div class="col-md-8 col-md-offset-2" ng-controller="TestController">
		<div ng-click="Pop()">{{sometext}}</div>
		<div id="userList"></div>
	</div>
  <div class="col-md-2"></div>
</div>

<div class="row">
	<div class="col-md-8 col-md-offset-2" ng-controller="UsersController">
		<table id="angularUsers" class="table table-striped">
		<thead>
		  <tr>
			  <th>Name</th>
			  <th>Email</th>
			  <th>Location</th>
		  </tr>
		</thead>
		<tbody>
		    <tr ng-repeat="user in users" data-id="{{user.id}}">
		      <td>{{user.first_name}} {{user.last_name}}</td>
		      <td>{{user.email}}</td>
		      <td>{{user.address_text}}</td>
		    </tr>
		</tbody>
		</table>
	</div>
  <div class="col-md-2"></div>
</div>
